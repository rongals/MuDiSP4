#
# if the command in OL is completed
# --> delete it
#

sp {crai*propose*delete-command
   (state <s> ^name crai
              ^io.output-link.command.status completed)
-->
   (<s> ^operator <op> + =)
   (<op> ^name delete-command)
}



sp {apply*delete-command
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<ol> ^command <cmd>)
   (<op> ^name delete-command)
-->
   (<ol> ^command <cmd> -)
}
