sp {propose*initialize-crai
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-crai)
}

sp {apply*initialize-crai
   (state <s> ^operator <op> ^io.input-link <il>)
   (<op> ^name initialize-crai)
-->
   (<s> ^name crai)
   (<il> ^fresh yes)
   (<il> ^carmap <cm>)
   (<cm> ^ncarrs 2 ^carr <c0> <c1>)
   (<c0> ^cid 0 ^used yes)
   (<c1> ^cid 1 ^used no)   
   (<il> ^usrmap <umap>)
   (<umap> ^nusers 2 ^maxerr 100 ^pstep (/ 1 10) ^user <u0> <u1>)
   (<u0> ^uid 0 ^requires 1 ^errs 0)
   (<u1> ^uid 1 ^requires 2 ^errs 0)
   (<il> ^channels <chs>)
   (<chs> ^coeff <c00> <c01> <c10> <c11>)
   (<c00> ^user <u0> ^carr <c0> ^value (/ 1 100))
   (<c01> ^user <u0> ^carr <c1> ^value (/ 1 100))
   (<c10> ^user <u1> ^carr <c0> ^value (/ 1 100))
   (<c11> ^user <u1> ^carr <c1> ^value (/ 1 100))
   (<il> ^allmap <als>)
   (<als> ^res <r0>)
   (<r0> ^user <u0> ^carr <c0> ^power (/ 9 10))
}

