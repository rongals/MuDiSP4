#!/bin/bash
export ECLIPSE_HOME=/usr/local/sandbox_ronga/eclipse
export SOAR_HOME=/usr/local/sandbox_ronga/SOAR/bin
export PATH=$PATH:$SOAR_HOME:$ECLIPSE_HOME
export LD_LIBRARY_PATH=$SOAR_HOME
alias vs='java -jar "$SOAR_HOME/VisualSoar.jar"'
alias sd='java -jar "$SOAR_HOME/SoarJavaDebugger.jar"'
echo "insert a new GSL_RNG_SEED followed by [ENTER]"
read tmp_seed
export GSL_RNG_SEED=$tmp_seed
echo "new seed is GSL_RNG_SEED=$GSL_RNG_SEED"

